import random
#Length is key to increased security
#Optimal length is over 12 characters
#Pseudo Random Integer between range so likelihood of repeats is low

size = random.randint(14,25)

lower = ("abcdefghijklmnopqrstuvwxyz")

upper = ("ABCDEFGHIJKLMNOPQRSTUVWXYZ")

numbers = ("0123456789")

symbols = ("!Â£$%^&*@+-=<>~()[];:/_,")

all = lower + upper + numbers + symbols

password = "".join(random.sample(all,size))

print(password)
